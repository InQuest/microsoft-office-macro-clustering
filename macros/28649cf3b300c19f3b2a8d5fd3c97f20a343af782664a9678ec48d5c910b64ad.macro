                    End If
                Next k
                'If j = 1 Then
                '    counter = counter + 1
                'End If
            End If
        Next j
        If Mid(JSONString, i, 2) = "}}" Or Mid(JSONString, i, 3) = "} }" Then 'Detected end of feature
            counter = counter + 1
        End If
    Next i
    'Rows(2).EntireRow.Delete
    
    RunMapChecks (copyRegionColumn)
    
