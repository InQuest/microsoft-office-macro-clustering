Sub AddMenus()

    Dim cMenu1 As CommandBarControl
    Dim cbMainMenuBar As CommandBar
    Dim iHelpMenu As Integer
    Dim cbcCutomMenu As CommandBarControl

    'MsgBox "Adding Menu..."
    'Delete any existing one, use On Error Resume next in case it does not exist.
    On Error Resume Next
    Application.CommandBars("Worksheet Menu Bar").Controls("&IP Functions").Delete

    'Set a CommandBar variable to Worksheet menu bar
    Set cbMainMenuBar = Application.CommandBars("Worksheet Menu Bar")

    'Return the Index number of the Help menu, used to place a custom menu before.
    iHelpMenu = cbMainMenuBar.Controls("Help").Index

    If iHelpMenu > 0 Then
        'Add a Control to the "Worksheet Menu Bar" before Help, set a CommandBarControl variable to it
        Set cbcCutomMenu = cbMainMenuBar.Controls.Add(Type:=msoControlPopup, Before:=iHelpMenu)
    Else
        'for non-English version of Excel
        Set cbcCutomMenu = cbMainMenuBar.Controls.Add(Type:=msoControlPopup)
    End If

    'Give the control a caption
    cbcCutomMenu.Caption = "&IP Functions"

    'Working with our new Control, add a sub control and give it a Caption and tell it which macro to run (OnAction).
    With cbcCutomMenu.Controls.Add(Type:=msoControlButton)
        .Caption = "About"
        .OnAction = "AboutRBIPFxSub"
    End With


    On Error GoTo 0

End Sub

Sub DeleteMenu()

    On Error Resume Next
    Application.CommandBars("Worksheet Menu Bar").Controls("&IP Functions").Delete
    On Error GoTo 0
End Sub
